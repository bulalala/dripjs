{
  "name": "drip",
  "version": "0.1.0",
  "description": "一个帮助交易者建立全面完善的交易系统的量化开源平台",
  "engines": {
    "node": ">= 10.15.0",
    "npm": ">= 6.4.1",
    "yarn": ">= 1.12.3"
  },
  "scripts": {
    "bdd": "yarn run jest --watch --onlyChanged",
    "coverage": "jest -c jest.config.js --collectCoverage",
    "ci:coverage": "jest -c jest.config.js --coverage --coverageReporters=text-lcov | coveralls",
    "format": "yarn run format:ts && yarn run format:md",
    "format:md": "prettier --config .prettierrc --write '{docs,modules,projects}/**/*.md' --parser markdown",
    "format:ts": "yarn run format:ts:import && prettier --config .prettierrc --write '{modules,projects}/**/*.ts'",
    "format:ts:import": "import-sort --write '{modules,projects}/**/*.ts'",
    "jest": "jest -c jest.config.js",
    "lint": "yarn run lint:md && yarn run lint:ts",
    "lint:md": "markdownlint **/*.md",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json",
    "test": "yarn run jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlq4863947/drip.git"
  },
  "keywords": [
    "trading platform"
  ],
  "author": "zlq4863947@gmail.com",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/zlq4863947/drip/issues"
  },
  "homepage": "https://github.com/zlq4863947/drip#readme",
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/node-fetch": "^2.1.6",
    "@types/qs": "^6.5.1",
    "@types/ws": "^6.0.1",
    "coveralls": "^3.0.2",
    "import-sort": "^5.2.0",
    "import-sort-cli": "^5.2.0",
    "import-sort-parser-typescript": "^5.0.0",
    "import-sort-style-module-alias": "^1.0.4",
    "jest": "23.6.0",
    "markdownlint": "^0.12.0",
    "markdownlint-cli": "^0.13.0",
    "prettier": "^1.16.4",
    "ts-jest": "23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3",
    "vrsource-tslint-rules": "^6.0.0",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "node-fetch": "^2.3.0",
    "rxjs": "^6.4.0",
    "ws": "^6.1.3"
  }
}
